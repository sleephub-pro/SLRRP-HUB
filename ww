local WindUI

do
    local ok, result = pcall(function()
        return require("./src/Init")
    end)
    
    if ok then
        WindUI = result
    else 
        WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()
    end
end

function createPopup()
    return WindUI:Popup({
        Title = "Welcome to the WindUI!",
        Icon = "bird",
        Content = "‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏™‡∏Ñ‡∏•‡∏¥‡∏õ‡πÑ‡∏°‡πà‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢ ‡πÉ‡∏ä‡πâ‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏£‡∏∞‡∏°‡∏±‡∏î‡∏£‡∏∞‡∏ß‡∏±‡∏á‡∏î‡πâ‡∏ß‡∏¢‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö",
        Buttons = {
            {
                Title = "‡πÄ‡∏Ç‡πâ‡∏≤‡πÉ‡∏à‡πÅ‡∏•‡πâ‡∏ß",
                Icon = "bird",
            },
        }
    })
end



-- */  Window  /* --
local Window = WindUI:CreateWindow({
    Title = "SLEEP HUB",
    Author = "by .ftgs ‚Ä¢ Footagesus",
    Folder = "ftgshub",
    Icon = "rbxassetid://121030902371363",
    IconSize = 22*2,
    NewElements = true,
    --Size = UDim2.fromOffset(700,700),
    
    HideSearchBar = false,
    
    OpenButton = {
        Title = " SLEEP HIB UI", -- can be changed
        CornerRadius = UDim.new(1,0), -- fully rounded
        StrokeThickness = 3, -- removing outline
        Enabled = true, -- enable or disable openbutton
        Draggable = true,
        OnlyMobile = false,
        
        Color = ColorSequence.new( -- gradient
            Color3.fromHex("#30FF6A"), 
            Color3.fromHex("#e7ff2f")
        )
    },
    KeySystem = {
        Title = "Key System Example  |  WindUI Example",
        Note = "Key System. Key: ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏î‡∏¢‡∏∑‡∏ô‡∏¢‡∏±‡∏ô‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢",
        KeyValidator = function(EnteredKey)
            if EnteredKey == "2" then
                createPopup()
                return true
            end
            return true
            -- return EnteredKey == "1234" -- if key == "1234" then return true else return false end
        end
    }
})






local Tab = Window:Tab({
    Title = "‡∏´‡∏ô‡∏±‡∏≤‡∏´‡∏•‡∏±‡∏Å",
    Icon = "bird", -- optional
    Locked = false,
})


-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô
local AmountInput = Tab:Input({
    Title = "Amount",
    Desc = "‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£",
    Placeholder = "‡πÉ‡∏™‡πà‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç...",
    Value = "1", -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
})

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Toggle ‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏£‡πà‡∏ß‡∏°‡∏Å‡∏±‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°
local Toggle = Tab:Toggle({
    Title = "Toggle",
    Desc = "Toggle Description",
    Icon = "bird",
    Type = "Checkbox",
    Value = false, -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    Callback = function(state)
        if state then -- ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î Toggle
            -- ‡∏î‡∏∂‡∏á‡∏Ñ‡πà‡∏≤‡∏à‡∏≤‡∏Å‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÅ‡∏•‡∏∞‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö
            local amount = tonumber(AmountInput.Value) or 1
            
            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡πá‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏ö‡∏ß‡∏Å
            if amount <= 0 then
                amount = 1
            end
            
            -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏î‡πâ‡∏ß‡∏¢‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡πÉ‡∏™‡πà
            local ecoRemote = game:GetService("ReplicatedStorage")
                :WaitForChild("E&F")
                :WaitForChild("Eco")
                :WaitForChild("AddEcoRE")
            
            -- ‡∏™‡πà‡∏á‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏õ‡∏£‡∏∞‡πÄ‡∏†‡∏ó‡∏î‡πâ‡∏ß‡∏¢‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô
            ecoRemote:FireServer("Body", amount)
            ecoRemote:FireServer("Arm", amount)
            ecoRemote:FireServer("Leg", amount)
            ecoRemote:FireServer("coin", amount)
            ecoRemote:FireServer("win", amount)
        end
    end
})




-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Textbox ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏Å‡πà‡∏≠‡∏ô
local AmountTextbox = Tab:Textbox({
    Title = "‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Potions",
    Desc = "‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏£‡∏±‡∏ö (‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç)",
    Icon = "plus",
    Value = "1", -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏õ‡πá‡∏ô 1
    Callback = function(value)
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏™‡πà
        local num = tonumber(value)
        if not num or num < 1 then
            warn("‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏™‡πà‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏ó‡∏µ‡πà‡∏°‡∏≤‡∏Å‡∏Å‡∏ß‡πà‡∏≤ 0")
            AmountTextbox:SetValue("1") -- ‡∏ï‡∏±‡πâ‡∏á‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏õ‡πá‡∏ô 1 ‡∏ñ‡πâ‡∏≤‡∏Ñ‡πà‡∏≤‡∏ú‡∏¥‡∏î
        end
    end
})

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏±‡∏ö Potions
local Toggle = Tab:Toggle({
    Title = "‡∏£‡∏±‡∏ö Potions",
    Desc = "‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏£‡∏±‡∏ö potions ‡∏ï‡∏≤‡∏°‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡∏±‡πâ‡∏á‡πÑ‡∏ß‡πâ",
    Icon = "bird",
    Type = "Checkbox",
    Value = false,
    Callback = function(state)
        if state then -- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î Toggle
            -- ‡∏î‡∏∂‡∏á‡∏Ñ‡πà‡∏≤‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏à‡∏≤‡∏Å Textbox
            local desiredAmount = tonumber(AmountTextbox.Value) or 1
            
            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÉ‡∏´‡πâ‡πÅ‡∏ô‡πà‡πÉ‡∏à‡∏ß‡πà‡∏≤‡∏Ñ‡πà‡∏≤‡πÑ‡∏°‡πà‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 1
            if desiredAmount < 1 then
                desiredAmount = 1
                warn("‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ñ‡∏π‡∏Å‡∏ï‡∏±‡πâ‡∏á‡πÄ‡∏õ‡πá‡∏ô 1 ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏™‡πà‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á")
            end
            
            -- ‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠ Potion ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            local potionIDs = {"BodyPotion", "LegPotion", "ArmPotion"}
            
            -- ‡∏™‡πà‡∏á request ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏õ‡∏£‡∏∞‡πÄ‡∏†‡∏ó
            for _, potionID in ipairs(potionIDs) do
                local args = {
                    {
                        Number = desiredAmount, -- ‡πÉ‡∏ä‡πâ‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏à‡∏≤‡∏Å Textbox
                        Type = "Potion",
                        ID = potionID
                    }
                }
                
                -- Fire RemoteEvent
                local claimRE = game:GetService("ReplicatedStorage"):WaitForChild("E&F"):WaitForChild("Reward"):WaitForChild("ClaimRewardRE")
                claimRE:FireServer(unpack(args))
                
                -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô rate limit
                wait(0.1)
            end
            
            -- ‡∏õ‡∏¥‡∏î Toggle ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏´‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à
            Toggle:SetValue(false)
        end
    end
})




-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ (‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô)
local ecoAmount = 10
local spinAmount = 1
local petAmount = 1

-- =========================================
-- ‡∏™‡πà‡∏ß‡∏ô‡∏ó‡∏µ‡πà 1: Eco Reward
-- =========================================
-- ‡∏Å‡∏•‡πà‡∏≠‡∏á‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Eco
local EcoInput = Tab:Input({
    Title = "Eco Reward Amount",
    Desc = "‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Eco reward",
    Value = "10",
    Callback = function(value)
        ecoAmount = tonumber(value) or 10
    end
})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡πà‡∏á Eco Reward
local EcoButton = Tab:Button({
    Title = "Claim Eco Reward",
    Desc = "‡∏™‡πà‡∏á Eco reward event ‡πÅ‡∏¢‡∏Å‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏≤‡∏Å",
    Icon = "coins",
    Callback = function()
        local args = {
            {
                Number = ecoAmount,
                Type = "Eco",
                ID = "golden"
            }
        }
        game:GetService("ReplicatedStorage"):WaitForChild("E&F"):WaitForChild("Reward"):WaitForChild("ClaimRewardRE"):FireServer(unpack(args))
    end
})

-- =========================================
-- ‡∏™‡πà‡∏ß‡∏ô‡∏ó‡∏µ‡πà 2: Spin Reward
-- =========================================
-- ‡∏Å‡∏•‡πà‡∏≠‡∏á‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Spin
local SpinInput = Tab:Input({
    Title = "Spin Reward Amount",
    Desc = "‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Spin reward",
    Value = "1",
    Callback = function(value)
        spinAmount = tonumber(value) or 1
    end
})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡πà‡∏á Spin Reward
local SpinButton = Tab:Button({
    Title = "Claim Spin Reward",
    Desc = "‡∏™‡πà‡∏á Spin reward event ‡πÅ‡∏¢‡∏Å‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏≤‡∏Å",
    Icon = "sync",
    Callback = function()
        local args = {
            {
                Number = spinAmount,
                Type = "Spin",
                ID = ""
            }
        }
        game:GetService("ReplicatedStorage"):WaitForChild("E&F"):WaitForChild("Reward"):WaitForChild("ClaimRewardRE"):FireServer(unpack(args))
    end
})







-- ‡∏™‡∏°‡∏°‡∏ï‡∏¥‡∏ß‡πà‡∏≤‡∏ô‡∏µ‡πà‡∏Ñ‡∏∑‡∏≠‡∏†‡∏≤‡∏¢‡πÉ‡∏ô Script ‡∏´‡∏£‡∏∑‡∏≠ LocalScript
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
local isWarping = false
local warpConnection = nil

local Toggle = Tab:Toggle({
    Title = "Auto Warp Boxes",
    Desc = "‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏Å‡∏•‡πà‡∏≠‡∏á‡πÉ‡∏ô workspace.Boxes ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥",
    Icon = "bird",
    Type = "Checkbox",
    Value = false,
    Callback = function(state)
        -- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
        if state then
            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏°‡∏µ Boxes ‡πÉ‡∏ô workspace ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
            if not workspace:FindFirstChild("Boxes") then
                warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö workspace.Boxes")
                return
            end
            
            isWarping = true
            print("‚úÖ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥...")
            
            -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ó‡∏∏‡∏Å object ‡πÉ‡∏ô Boxes
            warpConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if not isWarping then return end
                
                local boxesFolder = workspace.Boxes
                local allObjects = boxesFolder:GetChildren()
                
                if #allObjects == 0 then
                    warn("‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏ü‡∏•‡πå‡πÉ‡∏ô workspace.Boxes")
                    return
                end
                
                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á object ‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏ï‡∏±‡∏ß
                for _, obj in ipairs(allObjects) do
                    if not isWarping then break end -- ‡∏´‡∏¢‡∏∏‡∏î‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏õ‡∏¥‡∏î toggle
                    
                    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤ object ‡∏°‡∏µ Part ‡∏ó‡∏µ‡πà‡∏à‡∏∞‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÑ‡∏î‡πâ
                    local targetPart = obj
                    if obj:IsA("Model") then
                        targetPart = obj:FindFirstChild("HumanoidRootPart") or obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
                    end
                    
                    if targetPart and targetPart:IsA("BasePart") then
                        -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≠‡∏á object (‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢)
                        humanoidRootPart.CFrame = targetPart.CFrame * CFrame.new(0, 5, 0)
                        wait(0.5) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ
                    end
                end
                
                print("üîÑ ‡∏ß‡∏≤‡∏õ‡∏Ñ‡∏£‡∏ö‡∏ó‡∏∏‡∏Å‡πÑ‡∏ü‡∏•‡πå‡πÅ‡∏•‡πâ‡∏ß ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏´‡∏°‡πà...")
            end)
            
        else
            -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
            isWarping = false
            if warpConnection then
                warpConnection:Disconnect()
                warpConnection = nil
            end
            print("‚ùå ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥")
        end
    end
})


local Toggle = Tab:Toggle({
    Title = "Toggle",
    Desc = "Toggle Description",
    Icon = "bird",
    Type = "Checkbox",
    Value = false,
    Callback = function(state)
        if state then -- Only fire when turned ON
            local args = {"16"}
            game:GetService("ReplicatedStorage"):WaitForChild("E&F"):WaitForChild("Shovel"):WaitForChild("BuyShovelRE"):FireServer(unpack(args))
        end
    end
})
